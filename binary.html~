<script>
var tree=function()
{
this.root=null;
this.parent=null;
this.current=null;


                      var Node=function(value){
			this.value=value;
			this.left=null;
			this.right=null;
			}
 
};

	tree.prototype={
                  //var node=new Node();
		insert :function(value){
                 
			
	         //var node=new Node(value);
                  
		if(this.root==null){
                //var node=new Node(value);
		this.root=node;
		}
		else
		{
			node.current=this.root;
			node.parent=null;

			while(1){
			parent=current;
			if(this.value<parent.value){
			current=current.left;
			
			if(current==null){
                         parent.left=Node;
                          return;
			}
                        }
                        else{
                           current=current.right;
                           if(current==null){
                              parent.right=node;
                              return;
			}
                      }
                    }
}
          
}
                   var node=new Node(value);
}
    var temp=new tree();
    temp.insert(10);
    temp.insert(5);
    temp.insert(3);
    temp.insert(7);
    temp.insert(2);
    conslole.log(temp.root);
</script>

